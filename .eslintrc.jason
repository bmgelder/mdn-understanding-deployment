{
    "env": {
        "es6": true,
        "browser": true;
    },
    "extends": ["eslint:recommended", "plugin: react/recommended"],
    "parseOptions" : {
        "ecmaVersion": 6,
         "sourceType": "module",
         "ecmaFaetures": {
             "jsx": true
         }
    },
    "plugins": ["react"],
    "rules": {
        "semi": "error",
        "no-console": 0,
        "react/jsx-uses-vars": "error"
    }
}